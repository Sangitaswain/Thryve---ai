{
  "name": "Thryve Wellness",
  "description": "Mobile preventive healthcare app built with React Native and Gemini AI.",
  "requestFramePermissions": [
    "camera",
    "microphone"
  ],
  "expo": {
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.thryve.wellness",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.thryve.wellness",
      "permissions": [
        "VIBRATE",
        "INTERNET"
      ]
    }
  }
}